{
    "name": "wallpaper_and_skins",
    "version": "1.0.3",
    "description": "Live Wallpaper and cute skins for computers",
    "main": "main.js",
    "productName": "Wallpaper and Skins",
    "scripts": {
        "start": "electron .",
        "dir": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "26032001",
        "productName": "Wallpaper and Skins",
        "copyright": "Copyright ©2024 TrongSaDoa",
        "directories": {
            "buildResources": "resources",
            "output": "dist/wallpaper_and_skins"
        },
        "allowToChangeInstallationDirectory": true,
        "mac": {
            "category": "Screen"
        },
        "win": {
            "target": "NSIS",
            "icon": "icon.png"
        }
    },
    "type": "module",
    "keywords": [
        "Screen",
        "live screen",
        "Wallpaper",
        "Skins computers",
        "Computers",
        "Electron"
    ],
    "author": "Quang Trọng",
    "license": "CC0-1.0",
    "repository": "https://github.com/quangtrong1506/wallpaper-and-skins",
    "publish": {
        "provider": "github",
        "releaseType": "release"
    },
    "devDependencies": {
        "electron": "^28.2.0",
        "electron-builder": "^24.9.1",
        "electron-packager": "^17.1.2"
    },
    "dependencies": {
        "common-es": "^1.0.1",
        "electron-is-dev": "^3.0.1",
        "electron-json-storage": "^4.6.0",
        "electron-updater": "^6.1.7",
        "fs-extra": "^11.2.0"
    }
}
