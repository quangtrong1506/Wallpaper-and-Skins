{
    "name": "live_screen",
    "version": "1.0.0",
    "description": "Live Wallpaper được tạo bởi Quang Trọng",
    "main": "main.js",
    "productName": "Live Wallpaper",
    "scripts": {
        "start": "electron .",
        "dir": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "15.06.2001",
        "productName": "Live Wallpaper",
        "copyright": "Copyright ©2023 QuangTrong",
        "directories": {
            "buildResources": "resources",
            "output": "dist/LiveScreen"
        },
        "mac": {
            "category": "Screen"
        },
        "win": {
            "target": "NSIS",
            "icon": "icon.png"
        },
        "extraFiles": [
            {
                "from": "src/images/build/",
                "to": "resources/images/",
                "filter": [
                    "**/*"
                ]
            }
        ]
    },
    "type": "module",
    "keywords": [
        "Screen",
        "live screen"
    ],
    "author": "Quang Trọng",
    "license": "CC0-1.0",
    "repository": "https://github.com/quangtrong1506/wallpaper-app-2",
    "publish": {
        "provider": "github",
        "releaseType": "release"
    },
    "devDependencies": {
        "electron": "^28.2.0",
        "electron-builder": "^24.9.1",
        "electron-packager": "^17.1.2"
    },
    "dependencies": {
        "brightness": "^3.0.0",
        "electron-is-dev": "^3.0.1",
        "electron-updater": "^6.1.7",
        "file-saver": "^2.0.5",
        "node-cmd": "^5.0.0",
        "node-gyp": "^10.0.1"
    }
}
